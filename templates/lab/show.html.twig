{% extends 'adminMenu.html.twig' %}

{% block title %}Lab{% endblock %}

{% block body %}
    <h1>Lab</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ lab.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ lab.name }}</td>
            </tr>
            <tr>
                <th>Identifier</th>
                <td>{{ lab.identifier }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ lab.description|raw }}</td>
            </tr>
            <tr>
                <th>CreatedTime</th>
                <td>{{ lab.createdTime ? lab.createdTime|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>UpdateTime</th>
                <td>{{ lab.updateTime ? lab.updateTime|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('lab_index') }}">back to list</a>

    <a href="{{ path('lab_edit', {'id': lab.id}) }}">edit</a>

    {{ include('lab/_delete_form.html.twig') }}
{% endblock %}
